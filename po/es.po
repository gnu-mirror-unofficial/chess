msgstr "mi puntuación = %d, puntuación del adversario = %d\n"

#: src/frontend/cmd.cc:696 src/frontend/cmd.cc:769
#, c-format
msgid "No moves to undo!\n"
msgstr "¡No hay ningún movimiento que deshacer!\n"

#: src/frontend/cmd.cc:707
#, c-format
